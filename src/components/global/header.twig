<mj-section mj-class="#fff" padding="20px 20px 30px">
  <mj-column>
    <mj-image src="{{ development ? server }}/assets/logo.svg" width="130px">
  </mj-column>
</mj-section>

<mj-section background-color="{{ style.color['grey-light'] }}">
  <mj-column padding="10px 20px">
    <mj-navbar
      base-url="/"
      hamburger="hamburger"
      ico-color="#000"
    >
      {% for element in data.nav %}
        <mj-navbar-link href="{{ element.url }}">
          {{ element.name }}
        </mj-navbar-link>
      {% endfor %}
    </mj-navbar>
  </mj-column>
</mj-section>